{
    "database_file_path": "database/database.sqlite3",
    "datetime_format": "%d.%m.%Y %H:%M",
    "handlers": [
        "cancel",
        "language",
        "languages",
        "menu",
        "profile",
        "start"
    ],
    "locale_default": "ru",
    "locales_folder_path": "locales/{locale}",
    "log_format": "<green>{time:YYYY-MM-DD HH:mm:ss.SSS}</green> | <level>{level: <8}</level> | <level>{message}</level>",
    "log_level_file": "INFO",
    "log_level_std": "TRACE",
    "log_files_path": "logs/{time:YYYY.MM.DD HH-mm-ss}.log",
    "parse_mode": "HTML"
}
