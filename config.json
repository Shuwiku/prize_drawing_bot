{
    "database_file": "database/database.sqlite3",
    "locale_data": "locales/data.txt",
    "locale_default": "en",
    "locale_folder": "locales/{locale}",
    "logger": {
        "log_format": "<green>{time:YYYY-MM-DD HH:mm:ss.SSS}</green> | <level>{level: <8}</level> | <level>{message}</level>",
        "log_level": {
            "file": "INFO",
            "std": "TRACE"
        },
        "log_path": "logs/{time:YYYY.MM.DD HH-mm-ss}.log"
    }
}
